getSymbols("^GSPC")
GSPC$chOPC<-ifelse((GSPC[,1]-lag(GSPC[,4],1))>0,1,0)#OPEN>prev CLOSE==chOPC
GSPC$chCO<-ifelse((GSPC[,4]-GSPC[,1])>0,1,0)#CLOSE-OPEN>0==chCO
GSPC$chCPC<-ifelse((GSPC[,4]-lag(GSPC[,4],1))>0,1,0)#CLOSE>prev CLOSE==chCPC
SI<-GSPC$chOPC
SI$CO<-GSPC$chCO
SI$CPC<-GSPC$chCPC

