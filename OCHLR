OCHLR<-function(ts){
O<-Op(ts)
C<-Cl(ts)
H<-Hi(ts)
L<-Lo(ts)
l<-length(L)
len<-(round(l/100)-1)*100
open<-O[(l-len+1):l]
close<-C[(l-len+1):l]
high<-H[(l-len+1):l]
low<-L[(l-len+1):l]
open<-O[(l-len+1):l]
open<-O[(l-len+1):l]
ret<-returns(C)
ret<-ret[(l-len+1):l]
OCHLR<-cbind(open,close,high,low,ret)
return(OCHLR)
}
